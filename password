import random
generated_password = ""
password = "+-/*!&$#?=@abcdefghijklnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890"
length = int(input("Введите длину пароля: "))
if length > 0:
    for i in range(length):
        generated_password += random.choice(password)
    print("Ваш сгенерированный пароль:", generated_password)
else:
    print("Длина пароля должна быть положительным числом.")
